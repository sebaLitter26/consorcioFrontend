{
  "name": "rrhh-frontend-web",
  "version": "1.0",
  "scripts": {
    "start": "ng serve --watch --port 4200 -o",
    "dev": "ng build -c=development --output-path=dev",
    "sta": "ng build -c=staging --output-path=sta",
    "prod": "ng build -c=production --output-path=prod",
    "pre-prod": "ng build -c=preProduction --output-path=pre-prod",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-dist": "ng build -c=production --stats-json --output-path=dist --named-chunks",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "bundle-analyzer": "npm run bundle-dist && npm run bundle-report",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "major": "npm version major",
    "minor": "npm version minor",
    "patch": "npm version patch",
    "pmajor": "npm version premajor --preid=build --git-tag-version=false",
    "pminor": "npm version preminor --preid=build --git-tag-version=false",
    "ppatch": "npm version prepatch --preid=build --git-tag-version=false",
    "release": "npm version prerelease --preid=build --git-tag-version=false",
    "rcmajor": "npm version premajor --preid=rc --git-tag-version=false",
    "rcminor": "npm version preminor --preid=rc --git-tag-version=false",
    "rcpatch": "npm version prepatch --preid=rc --git-tag-version=false",
    "rc": "npm version prerelease --preid=rc --git-tag-version=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.0.3",
    "@angular/cdk": "^15.0.2",
    "@angular/common": "~15.0.3",
    "@angular/compiler": "~15.0.3",
    "@angular/core": "~15.0.3",
    "@angular/forms": "~15.0.3",
    "@angular/material": "^15.0.2",
    "@angular/platform-browser": "~15.0.3",
    "@angular/platform-browser-dynamic": "~15.0.3",
    "@angular/router": "~15.0.3",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "chart.js": "^4.0.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.29.3",
    "hex-to-css-filter": "^5.4.0",
    "jsbarcode": "^3.11.5",
    "material-icons": "^1.13.1",
    "ngx-mask": "^14.2.4",
    "node-forge": "^1.3.1",
    "rxjs": "~7.6.0",
    "tslib": "^2.4.1",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular/cli": "~15.0.3",
    "@angular/compiler-cli": "~15.0.3",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^18.11.13",
    "@types/node-forge": "^1.3.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
